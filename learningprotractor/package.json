{
 "name": "learningprotractor",
 "dependencies": {
  "protractor": "^5.3.0",
  "jasmine-data-provider": "^2.2.0",
  "protractor-jasmine2-html-reporter": "0.0.7"
 },
 "scripts": {
  "webdriver-update": "./node_modules/.bin/webdriver-manager update",
  "webdriver-start": "./node_modules/.bin/webdriver-manager start",
  "configfile": "./node_modules/.bin/protractor config.js",
  "smoke": "./node_modules/.bin/protractor config.js --suite smoke",
  "regression": "./node_modules/.bin/protractor config.js --suite regression",
  "start": "npm run webdriver-update &&npm run webdriver-start &&npm run configfile"
 }
}
